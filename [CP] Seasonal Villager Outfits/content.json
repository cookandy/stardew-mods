    "ConfigSchema":
    {
        "SwimSuitsEnabled":
        {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "Enables the villagers wearing swimsuits when at the beach or during beach-location festivals. Default is true."
        },
        "EnableMermaidPendants":
        {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "Enables mermaid pendant overlays for engaged/married NPCs. Portraits only. Default is true."
        },
        "EnableImagEdContent":
        {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "Requires ImagEd. If you have ImagEd installed masks and greyscales will be automatically be used. Use this to toggle them off. Default is true."
        },
        "WeddingStyle":
        {
            "AllowValues": "VCSO, DesertDove, SCS, Vanilla",
            "Default": "DesertDove",
            "Description": "The style of wedding outfit. See the wiki/list of options for previews. Default is DesertDove."
        },
        "EnableFlowerQueen":
        {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "Allows other NPCs to be crowned flower queen besides Haley. Default is true."
        },
        "AllowBachelorsToBeFlowerKing":
        {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "Allows the bachelors to be crowned flower king. Default is true."
        },
        "EnableDialogueChanges":
        {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "Enables minor dialogue changes to reflect what characters are wearing. Default is true."
        },
        "EnableSleepwear":
        {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "If enabled, spouses will sometimes wear their sleepwear until 11:30AM around the farmhouse. Default is true."
        },

        // Seasonal Villager Outfits 

        // Emily

        "EmilyTattoos":
        {
            "AllowValues": "true, false",
            "Default": "false",
            "Description": "Enables tattoo overlays for Emily. Default is false."
        },

        // Haley

        "HaleyCowPhotoVariant":
        {
            "AllowValues": "Seasonal, Vanilla",
            "Default": "Seasonal",
            "Description": "Changes Haley's 8-heart event photos to be seasonal or vanilla. Default is seasonal."
        },

        // Harvey

        "HarveyFacialHair":
        {
            "AllowValues": "Mustache, Shaved",
            "Default": "Mustache",
            "Description": "Keeps Harvey's mustache or shaves it off. Default is mustache."
        },
        "HarveyEyeColor":
        {
            "AllowValues": "Green, Brown",
            "Default": "Green",
            "Description": "Changes the color of Harvey's eyes. Default is green."
        },

        // Krobus

        "KrobusEnabled":
        {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "Enables outfits for Krobus. Default is true."
        },

        // Maru - Effects the Character sprite only.

        "MaruGlasses":
        {
            "AllowValues": "true, false",
            "Default": "false",
            "Description": "Enables glasses overlays for Maru. Character only. Default is false."
        },

        // Morris

        "MorrisEnabled":
        {
            "AllowValues": "true, false",
            "Default": "false",
            "Description": "Enable SVO for Morris. Default is false."
        },

        // Sandy

        "SandyEnabled":
        {
            "AllowValues": "true, false",
            "Default": "false",
            "Description": "Enable SVO for Sandy. Default is false."
        },

        // Sebastian

        "SebastianGlasses":
        {
            "AllowValues": "true, false",
            "Default": "false",
            "Description": "Enables glasses overlay for Sebastian. Can toggle both glasses & piercings. Default is false."
        },
        "SebastianPiercings":
        {
            "AllowValues": "true, false",
            "Default": "false",
            "Description": "Enables piecings over for Sebastian. Can toggle both glasses & piercings. Default is false."
        },

        // Wizard

        "WizardEnabled":
        {
            "AllowValues": "true, false",
            "Default": "false",
            "Description": "Enable SVO for the Wizard. Default is false."
        },

        // Swimsuit Config

        // Linus Swimsuit

        "LinusSwimVariant":
        {
            "AllowValues": "Censored, GrassSkirt",
            "Default": "Censored",
            "Description": "Changes Linus's swim outfit to either be censored with a black bar or give him a grass skirt. Character only. Default is censored."
        },

        // Willy Swimsuit

        "WillySwimsuitEnabled":
        {
            "AllowValues": "true, false",
            "Default": "true",
            "Description": "Enables Willy's swimsuit. SFW but can look odd when fishing. Default is true."
        }
    },

