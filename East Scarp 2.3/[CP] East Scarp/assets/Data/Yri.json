{
    "Changes": [
        //Tilesheets
        {
            "Action": "EditImage",
            "PatchMode": "Replace",
            "Target": "Maps/{{season}}_cottage",
            "FromFile": "assets/TileSheets/Yri/{{season}}_cottage.png",
        },
        {
            "Action": "EditImage",
